image=config['image']
workdir: config['workdir']
output_path=config['output_path']
data_path=config['data_path']

run_wildcards=config.get('run_wildcards', '')

rule preprocessA:
	output:
		test_data='{output_path}/%s/test_data.pq' % run_wildcards,
		train_data='{output_path}/%s/train_data.pq' % run_wildcards
	shell:
		'echo "train data + {wildcards.aparam}" > {output.train_data}'
		'&& '
		' echo "test data + {wildcards.aparam}" > {output.test_data}'

